#include <stdio.h>

int main() {
    float celsius, fahrenheit;

    // Запрос ввода температуры в градусах Цельсия
    printf("Введите температуру в градусах Цельсия: ");
    scanf("%f", &celsius);

    // Преобразование в градусы Фаренгейта
    fahrenheit = (celsius * 9 / 5) + 32;

    // Вывод результата с двумя знаками после запятой
    printf("C = %.2f ; F = %.2f\n", celsius, fahrenheit);

    return 0;
}
